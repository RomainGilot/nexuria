// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Utilisateur {
  id        Int      @id @default(autoincrement())
  prenom    String
  nom       String
  permission String
}

model Client {
  id        Int     @id @default(autoincrement())
  prenom    String
  nom       String
  entreprise String
  email     String
  telephone String
}

